{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "types": ["node", "next", "next-auth"],
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [ "svgr.d.ts","next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/lib/actions.js", "src/components/chart/chart.tsx", "src/app/(auth)/signup/password/page.tsx", "src/app/(auth)/signup/email", "src/app/(auth)/signup/email/CameralIcon", "src/app/(auth)/signup/email/MailIcon"],
  "exclude": ["node_modules"],
}
